{
  "name": "demo-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start-public": "ng serve --host=0.0.0.0",
    "build-sit": "ng build --configuration=sit --outputPath=dist/sit --baseHref=/demo-angular/ && copyfiles .htaccess dist/sit",
    "build-uat": "ng build --configuration=uat --outputPath=dist/uat --baseHref=/demo-angular/ && copyfiles .htaccess dist/uat",
    "build-prd": "ng build --prod --outputPath=dist/prd --baseHref=/demo-angular/ && copyfiles .htaccess dist/prd"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.1",
